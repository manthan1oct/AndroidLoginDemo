<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="fontType" format="enum">
        <enum name="Light" value="0" />
        <enum name="Regular" value="1" />
        <enum name="Medium" value="2" />
        <enum name="Bold" value="3" />
    </attr>

    <declare-styleable name="FontTextView">
        <attr name="fontType" />
    </declare-styleable>


</resources>